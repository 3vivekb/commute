{
  "name": "@conveyal/commute",
  "description": "Commute analysis",
  "scripts": {
    "build": "mastarm build",
    "deploy": "mastarm deploy",
    "cover": "npm run test-all -- --coverage --coverage-paths \"client/**/*.js server/**/*.js\"",
    "cover-client": "npm run test-client -- --coverage --coverage-paths \"client/**/*.js\"",
    "cover-server": "npm run test-server -- --coverage --coverage-paths \"server/**/*.js\"",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "start": "npm run build && node server/development.js",
    "test": "npm run test-lint && npm run test-client && npm run test-server",
    "test-all": "mastarm test -c ./configurations/test -e test",
    "test-client": "mastarm test -c ./configurations/test -e test __tests__/client",
    "test-lint": "mastarm lint",
    "test-server": "mastarm test -e test __tests__/server",
    "watch": "npm-run-all --parallel watch:client watch:server",
    "watch:client": "mastarm build --watch",
    "watch:server": "nodemon --watch server server/development.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/conveyal/commute.git"
  },
  "keywords": [
    "Modeify",
    "tdm"
  ],
  "author": "Conveyal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conveyal/commute/issues"
  },
  "homepage": "https://github.com/conveyal/commute#readme",
  "devDependencies": {
    "buffer-equal-constant-time": "^1.0.1",
    "enzyme": "^2.6.0",
    "enzyme-to-json": "^1.3.0",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "fs-extra": "^0.30.0",
    "mastarm": "^3.0.0",
    "nock": "^9.0.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "pretty": "^1.0.0",
    "react-addons-test-utils": "^15.3.2",
    "redux-mock-store": "^1.2.1",
    "redux-promise": "^0.5.3",
    "rimraf": "^2.5.4",
    "semantic-release": "^4.3.5",
    "supertest": "^2.0.1",
    "supertest-as-promised": "^4.0.2"
  },
  "dependencies": {
    "@conveyal/woonerf": "^0.4.0",
    "async": "^2.1.4",
    "body-parser": "^1.15.2",
    "d3-dsv": "^1.0.3",
    "font-awesome": "^4.6.3",
    "humanize-duration": "^3.9.1",
    "isomorphic-mapzen-search": "^0.0.4",
    "leaflet": "^0.7.7",
    "lodash.isequal": "^4.4.0",
    "lodash.isnumber": "^3.0.3",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "lodash.uniqueid": "^4.0.1",
    "lonlng": "^0.2.0",
    "moment": "^2.15.2",
    "mongoose": "^4.6.4",
    "normalize.css": "^5.0.0",
    "qs": "^6.3.0",
    "rc-slider": "^5.2.1",
    "react": "^15.3.2",
    "react-addons-shallow-compare": "^15.3.2",
    "react-addons-update": "^15.3.2",
    "react-bootstrap": "^0.30.5",
    "react-bootstrap-table": "^2.5.6",
    "react-dom": "^15.3.2",
    "react-dropzone": "^3.7.2",
    "react-leaflet": "^0.12.3",
    "react-leaflet-cluster-layer": "^0.0.3",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "react-select": "^1.0.0-rc.2",
    "react-select-geocoder": "^0.2.1",
    "react-vis": "^0.6.4",
    "redux": "^3.6.0",
