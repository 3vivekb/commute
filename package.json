{
  "name": "@conveyal/commute",
  "description": "Commute analysis",
  "scripts": {
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "start": "npm-run-all --parallel start:client start:server",
    "start:client": "mastarm build client/index.js:assets/index.js client/index.css:assets/index.css --skip-check-dependencies --watch",
    "start:server": "nodemon --watch server server/development.js",
    "test": "mastarm lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/conveyal/commute.git"
  },
  "keywords": [
    "Modeify",
    "tdm"
  ],
  "author": "Conveyal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conveyal/commute/issues"
  },
  "homepage": "https://github.com/conveyal/commute#readme",
  "devDependencies": {
    "express": "^4.14.0",
    "express-graphql": "^0.5.4",
    "express-jwt": "^5.1.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "semantic-release": "^4.3.5"
  },
  "dependencies": {
    "graphql": "^0.7.2",
    "mastarm": "^1.2.0",
    "mongoose": "^4.6.4",
    "normalize.css": "^5.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-actions": "^0.12.0"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
