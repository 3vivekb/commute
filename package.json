{
  "name": "@conveyal/commute",
  "description": "Commute analysis",
  "scripts": {
    "build": "mastarm build client/index.js:assets/index.js client/index.css:assets/index.css",
    "cover": "npm test -- --coverage --coverage-paths \"client/**/*.js server/**/*.js\"",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "start": "npm-run-all --parallel start:client start:server",
    "start:client": "mastarm build client/index.js:assets/index.js client/index.css:assets/index.css --watch",
    "start:server": "nodemon --watch server server/development.js",
    "pretest": "node __tests__/test-utils/setup-teardown.js setup",
    "test": "mastarm lint && NODE_ENV=test mastarm test",
    "posttest": "node __tests__/test-utils/setup-teardown.js teardown"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/conveyal/commute.git"
  },
  "keywords": [
    "Modeify",
    "tdm"
  ],
  "author": "Conveyal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conveyal/commute/issues"
  },
  "homepage": "https://github.com/conveyal/commute#readme",
  "devDependencies": {
    "buffer-equal-constant-time": "^1.0.1",
    "enzyme": "^2.5.1",
    "enzyme-to-json": "^1.1.4",
    "express": "^4.14.0",
    "express-graphql": "^0.5.4",
    "express-jwt": "^5.1.0",
    "fs-extra": "^0.30.0",
    "mastarm": "^1.3.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "react-addons-test-utils": "^15.3.2",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.4",
    "semantic-release": "^4.3.5"
  },
  "dependencies": {
    "font-awesome": "^4.6.3",
    "graphql": "^0.7.2",
    "lodash.isequal": "^4.4.0",
    "moment": "^2.15.2",
    "mongoose": "^4.6.4",
    "normalize.css": "^5.0.0",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.5",
    "react-bootstrap-table": "^2.5.6",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "react-select-geocoder": "^0.2.1",
    "redux": "^3.6.0",
    "redux-actions": "^0.12.0",
    "uuid": "^2.0.3"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
